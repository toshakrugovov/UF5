<Window x:Class="AudioPlayer.HistoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AudioPlayer" xmlns:viewmodel="clr-namespace:AudioPlayer.ViewModel" d:DataContext="{d:DesignInstance Type=viewmodel:HistoryViewModel}"
        mc:Ignorable="d"
        Title="История прослушиваний" Height="450" Width="800">
    <Grid Background="Beige">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="97*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="703*"/>
            
        </Grid.ColumnDefinitions>
        <ListBox Grid.Column="2" ItemsSource="{Binding History}" IsSynchronizedWithCurrentItem="True"/>
    </Grid>
</Window>
